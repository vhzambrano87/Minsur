            function CalcularTMH_OB_Balanza() {
                debugger;
                var var_h_op_ob = document.getElementById("h_op_ob").value.replace(",", ".");
                var var_ton_bal_faja = document.getElementById("ton_bal_faja").value.replace(",", ".");
                var rpta;
                if (var_h_op_ob.trim() == "" || var_h_op_ob.trim() == "0") {
                    document.getElementById("tmh_ob_bal").value = 0;

                }
                else {
                    rpta = (var_ton_bal_faja / var_h_op_ob);
                    document.getElementById("tmh_ob_bal").value = rpta.toString().replace(".", ",");
                }
            }

