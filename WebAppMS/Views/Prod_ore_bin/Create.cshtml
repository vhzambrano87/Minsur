@section pagespecific {
    <script type="text/javascript">

            // DO NOT REMOVE : GLOBAL FUNCTIONS!

            $(document).ready(function () {
                $('body').css('zoom', '80%');
                document.getElementById("fecha_text").value = document.getElementById("fecha_desc").value;

                if (document.getElementById("horas").value < 10) {
                    document.getElementById("hora_hora_op_ob").value = "0" + parseInt(document.getElementById("horas").value);
                }
                else {
                    document.getElementById("hora_hora_op_ob").value = document.getElementById("horas").value;
                }
                if (document.getElementById("minutos").value < 10) {
                    document.getElementById("hora_min_op_ob").value = "0" + parseInt(document.getElementById("minutos").value);
                }
                else {
                    document.getElementById("hora_min_op_ob").value = document.getElementById("minutos").value;
                }
                if (document.getElementById("horas_mantto").value < 10) {
                    document.getElementById("hora_hora_mantto_ob").value = "0" + parseInt(document.getElementById("horas_mantto").value);
                }
                else {
                    document.getElementById("hora_hora_mantto_ob").value = document.getElementById("horas_mantto").value;
                }
                if (document.getElementById("minutos_mantto").value < 10) {
                    document.getElementById("hora_min_mantto_ob").value = "0" + parseInt(document.getElementById("minutos_mantto").value);
                }
                else {
                    document.getElementById("hora_min_mantto_ob").value = document.getElementById("minutos_mantto").value;
                }
                if (document.getElementById("horas_operacion").value < 10) {
                    document.getElementById("hora_hora_operacion").value = "0" + parseInt(document.getElementById("horas_operacion").value);
                }
                else {
                    document.getElementById("hora_hora_operacion").value = document.getElementById("horas_operacion").value;
                }
                if (document.getElementById("minutos_operacion").value < 10) {
                    document.getElementById("hora_min_operacion").value = "0" + parseInt(document.getElementById("minutos_operacion").value);
                }
                else {
                    document.getElementById("hora_min_operacion").value = document.getElementById("minutos_operacion").value;
                }

                
                $('#hora_hora_op_ob').on("change", function () {
                    CalcularTMH_OB_Balanza();
                });
                $('#hora_min_op_ob').on("change", function () {
                    CalcularTMH_OB_Balanza();
                });


                $('#tm_acum_ini_ob').on("change", function () {
                    CalcularTonBalFaja();
                    CalcularTMH_OB_Balanza();
                });
                $('#tm_acum_fin_ob').on("change", function () {
                    CalcularTonBalFaja();
                    CalcularTMH_OB_Balanza();
                });


                $('#ton_bal_faja').on("change", function () {
                    CalcularTMH_OB_Balanza();
                });
                $('#hora_hora_operacion').on("change", function () {
                    CalcularDuracion();
                });
                $('#hora_min_operacion').on("change", function () {
                    CalcularDuracion();
                });

                document.getElementById("h_op_ob").value = document.getElementById("h_op_ob_desc").value;
                document.getElementById("h_mantto_ob").value = document.getElementById("h_mantto_ob_desc").value;
                document.getElementById("h_operacion").value = document.getElementById("h_operacion_desc").value;

            });


            function ObtenerCombos() {
                debugger;
                document.getElementById("turno_id").value = document.getElementById("ListaTurno123").value;
                document.getElementById("fecha_desc").value = document.getElementById("fecha_text").value;
                
                document.getElementById("ton_bal_faja_desc").value = document.getElementById("ton_bal_faja").value;
                document.getElementById("tmh_ob_bal_desc").value = document.getElementById("tmh_ob_bal").value;


                document.getElementById("h_op_ob_desc").value = document.getElementById("h_op_ob").value;
                document.getElementById("h_mantto_ob_desc").value = document.getElementById("h_mantto_ob").value;
                document.getElementById("h_operacion_desc").value = document.getElementById("h_operacion").value;

            }
            function CalcularDuracion() {

                document.getElementById("h_op_ob").value = parseInt(document.getElementById("hora_hora_op_ob").value) + (document.getElementById("hora_min_op_ob").value / 60);
                document.getElementById("h_mantto_ob").value = parseInt(document.getElementById("hora_hora_mantto_ob").value) + (document.getElementById("hora_min_mantto_ob").value / 60);
                document.getElementById("h_operacion").value = parseInt(document.getElementById("hora_hora_operacion").value) + (document.getElementById("hora_min_operacion").value / 60);

                document.getElementById("h_op_ob").value = document.getElementById("h_op_ob").value.replace(".", ",");
                document.getElementById("h_mantto_ob").value = document.getElementById("h_mantto_ob").value.replace(".", ",");
                document.getElementById("h_operacion").value = document.getElementById("h_operacion").value.replace(".", ",");

                document.getElementById("horas").value = document.getElementById("hora_hora_op_ob").value;
                document.getElementById("minutos").value = document.getElementById("hora_min_op_ob").value;

                document.getElementById("horas_mantto").value = document.getElementById("hora_hora_mantto_ob").value;
